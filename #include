#include <stdio.h>

int main() {
    /*
    icremento (++)
    pre-incremento ++a
    pos-incremento a++
    decremento (--)
    pre-decremento --a
    pos-decremento a--
    */

    int numero1 = 1;

    printf ("antes incremento: %d\n", numero1);
    numero1++;
    printf ("apos incremento: %d\n", numero1);


}
